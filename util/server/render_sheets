# /etc/cron.d/

### render sheets
* * * * * root cd /vol/drafty_source/drafty/util/ && python3 build_spreadsheet.py --host=localhost --database=csprofessors csprofessors.handlebars
#* * * * * root cd /vol/drafty_source/drafty/util/ && python3 build_spreadsheet.py --host=localhost --database=2300profs 2300profs.hbs

### edit history
* * * * * root cd /vol/drafty_source/drafty/util/ && python3 build_edit_history.py --host=localhost --database=csprofessors csprofessors.handlebars

### csv version
* * * * * root cd /vol/drafty_source/drafty/util/ && python3 gen_data_csv.py --database csprofessors

### regen data for CSOpenRankings
* * * * * root cd /vol/csopenrankings/backend/generators/ && python3 makeAllJson.py 
* * * * * root cd /vol/csopenrankings/backend/generators/ && python3 create_static_table.py

### run goaccess command in /vol/static
* */4 * * * root cd /vol/drafty_source/drafty/util/server/ && ./run_goaccess

### generate a databait from the system
* */24 * * * root cd /vol/DataBaits/databaits-api && python3 generate_databaits.py --csv "drafty_source/drafty/backend/data_sharing/csprofessors.csv" --tweet 
